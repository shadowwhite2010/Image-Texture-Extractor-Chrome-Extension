{
  "name": "Texture Extractor",
  "version": "1.0",
  "description": "Extract Texture from image",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["index.js"]
    }
  ],
  "action": {
    "default_icon": "stego_logo.png",
    "default_popup": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
   
  }
    
}